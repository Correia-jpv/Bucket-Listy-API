{
  "name": "bucketlistyapi",
  "version": "1.0.0",
  "description": "Node.js Restful CRUD API with Node.js, Express and MongoDB",
  "main": "server.js",
  "scripts": {
    "start": "npm run generate-jsdoc",
    "generate-jsdoc": "jsdoc -c ./documentation/jsdoc/jsdoc.json -t ./node_modules/ink-docstrap/template -R ./documentation/jsdoc/README.md -r .",
    "postgenerate-jsdoc": "node server.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "rest",
    "api"
  ],
  "author": "correia-jpv",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.6.10",
    "loginradius-sdk": "^11.3.0",
    "mongoose": "^6.6.4",
    "sanitize-html": "^2.7.0",
    "swagger-ui-express": "^4.3.0",
    "yamljs": "^0.3.0"
  }
}